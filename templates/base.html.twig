        <link href="https://fonts.googleapis.com/css2?family=Caveat+Brush&family=Roboto:wght@300&display=swap" rel="stylesheet">

        <!-- Stylesheets -->
        <link rel="stylesheet" href="{{ asset('assets/css/styles.css') }}">
    </head>
    <body>
